# hooks addon for printer support

function add_lp_modules() {
   force_load parport_pc
   force_load usblp
   force_load lp
}




if [ $TCOS_PRINTER ] ;then
    
    stat_before

    if [ -e /usr/bin/lp_server ]; then
       cpifexists /usr/bin/lp_server         /usr/bin/
    else
       cpifexists ${TCOS_BINS}/print_server  /usr/bin/
    fi
       add_lp_modules

    stat_after "Print server"
    
fi
# end of TCOS_PRINTER
