#!/bin/bash
set -e


if [ "$1" = "purge" ]; then

  # delete all generated files and dirs
  rm -rf /var/lib/tcos/tftp


  # stop tcos-standalone script
  if [ -x "`which invoke-rc.d 2>/dev/null`" ]; then
     invoke-rc.d tcos-standalone stop
  else
    /etc/init.d/tcos-standalone stop
  fi

fi



