#!/bin/sh
#

PREREQ=""

prereqs()
{
        echo "$PREREQ"
}

case $1 in
# get pre-requisites
prereqs)
        prereqs
        exit 0
        ;;
esac

quiet=n
. /scripts/functions
. /conf/tcos.conf
. /conf/tcos-run-functions


if [ -x /usr/sbin/listener-daemon.sh ]; then
   _log "MOUNT-listener trying to start"
   /usr/sbin/listener-daemon.sh > /var/log/listener-daemon.log 2>&1 &
fi

exit 0
